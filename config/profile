# start ssh agent and add git key automatically
# otherwise, you'd need to enter these commands manually each session
eval "$(ssh-agent -s)"
ssh-add ~/.ssh/id_github
. "$HOME/.cargo/env"

# Start the SSH agent if it's not already running
if [ -z "$SSH_AUTH_SOCK" ]; then
    eval "$(ssh-agent -s)" > /dev/null
    ssh-add /home/pickle/.ssh/id_github
fi
